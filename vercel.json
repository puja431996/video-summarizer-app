{
  "version": 2,
  "build": {
    "env": {
      "PYTHON_VERSION": "3.9.18"
    },
    "command": "pip install -r requirements.txt",
    "directory": "./"
  },
  "functions": {
    "app.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "index.html"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}